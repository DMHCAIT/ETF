{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "DEVELOPMENT_MODE": "false",
    "USE_MOCK_DATA": "false",
    "LIVE_TRADING_ENABLED": "true"
  }
}
